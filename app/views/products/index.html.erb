<article class="container">
  <h2>Products</h2>
  <hr>
  <div class="container-fluid prod">
    
      <% @products.each do |product| %>
        <%= render product %>
      <% end %>
      
  </div>
  
  <hr>
  <%= link_to 'New Product', new_product_path , class: "btn btn-default" %>
  <div class ="page-number">
    <%= will_paginate @products %>
  </div>
</article>



